{
  "name": "@livelybone/simple-observer",
  "version": "4.1.0",
  "description": "A simple implement of observer model and publish-subscribe model",
  "main": "./lib/umd/index.js",
  "module": "./lib/es/index.js",
  "unpkg": "./lib/umd/index.js",
  "scripts": {
    "build": "rimraf ./lib && cross-env NODE_ENV=production rollup -c",
    "eslint": "eslint ./src",
    "test": "npm run build && mocha ./test/index.spec.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "npx git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/livelybone/simple-observer.git"
  },
  "keywords": [
    "simple",
    "small",
    "subject",
    "observer",
    "subscribe",
    "subscriber",
    "publish",
    "publisher",
    "publish-subscribe"
  ],
  "author": "livelybone(2631541504@qq.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/livelybone/simple-observer/issues"
  },
  "homepage": "https://github.com/livelybone/simple-observer#readme",
  "devDependencies": {
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "^2.0.21",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.3.0",
    "mocha": "^5.2.0",
    "rollup": "^1.15.6",
    "rollup-plugin-license": "^0.9.0",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
